super + s
	screenshots --now;

super + shift + s
	screenshots --sel;

super + alt + s
	screenshots --in10;

# Reload sxhkd
super + Escape
	pkill -USR1 -x sxhkd;

# ranger 
#
super + e
	OpenApps --TfileManager;

#floating Terminal

super + shift + Return 
	OpenApps --floating;

#open rofi powermenu
super + BackSpace
	powermenu;

#open clipcat menu
control + alt + h
  OpenApps --clipcat;
